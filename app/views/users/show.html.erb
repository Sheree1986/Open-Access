<div class="container">
  <div class="row">
    <div class="col-md-4">
      <div class="card card-block">
        <h4 class="card-title"><%= @user.username %></h4>
        <p class="card-text"><%= @user.name %></p>
    
      </div>
    </div>
    <div class="col-md-8">
      <ul class="list-group">
        <% @blogs.each do |blog| %>
          <%= link_to blog, class: "list-group-item list-group-item-action" do %>
           
            <h5 class="list-group-item-heading"><%= blog.user.username %></h5>
            <p class="list-group-item-text"><%= blog.content %></p>
          <% end %>
        <% end %>
      </ul>
    </div>
  </div>
</div>